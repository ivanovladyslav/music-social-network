include ../layout/layout
div.container.with-top-padding
  h1 Attach track
  form.form-add-project(method='post', action='/addtracks_submit/' enctype="multipart/form-data")
    div.form-add-project-info
      input.form-control(name='releaseid' value="#{releaseid}" style="display:none;")
      input.form-control(type='text', name='title' placeholder='title')
      input(type='file' name='audio')
      
    div.contributors
      a.btn.btn-secondary.btn-sm(id="addContributorButton" onclick="addContributor()") Add contributor
      
    input.btn.btn-primary(type='submit', value='yes')
        
